<div class="container eligibilities">

  <h1> Age Eligibility </h1>

  <table>

    <thead class="table-info">
      <tr>
        <th colspan="3" >Section</th>
        <th>Date of birth till</th>
        <th>Age</th>
        <th>Date as on</th>
      </tr>
    </thead>

    <tbody>
      <% @eligibilities.each do |eligibility| %>
        <tr>
          <td colspan="3"><%= eligibility.section %></td>
          <td><%= eligibility.dateofbirthtill %></td>
          <td><%= eligibility.age %></td>
          <td><%= eligibility.dateason %></td>
          <% if admin? %>
          <td><%= link_to 'Show', eligibility %></td>
          <td><%= link_to 'Edit', edit_eligibility_path(eligibility) %></td>
          <td><%= link_to 'Destroy', eligibility, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          <% end %>
        </tr>
      <% end %>
    </tbody>

  </table>

  <br>

  <% if admin? %>

  <%= link_to 'New Eligibility', new_eligibility_path %>

  <% end %>

</div>

<%= render 'welcome/footer' %>
