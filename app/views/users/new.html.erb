<% provide(:title, 'Sign up') %>
<% provide(:button_text, 'Create my account') %>

<% if logged_in? %>
    <%= render 'welcome/tab' %>
  <% else %>

  <div class="signup">
    <div class="col-md-9 col-md-offset-3 container">
      <h1>Sign up</h1>
      <%= render 'form' %>
    </div>
  </div>

<% end %>
